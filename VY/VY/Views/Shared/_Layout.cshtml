<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>@ViewBag.Title</title>


    <link href="~/Content/bootstrap-litera.css" rel="stylesheet" type="text/css" />


    <link href="~/Content/bootstrap-datepicker3.standalone.css" rel="stylesheet" type="text/css" />

    <link href="~/Content/jquery.timepicker.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/feilmeldinger.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/modernizr-2.8.3.js"></script>
    <script src="~/Scripts/select2.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.js"></script>

    <script src="~/Scripts/bootstrap-datepicker.js"></script>
    <script src="~/Scripts/jquery.timepicker.js"></script>

    <link href="~/Content/css/select2.min.css" rel="stylesheet" type="text/css" />



</head>
<body>

    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <a class="navbar-brand " href="~/Home/Index">
            <img src="~/Content/images/VyLogo.png.png" style="height:50px" alt="" class="mr-5 pr-1">

        </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarColor03" aria-controls="navbarColor03" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarColor03">
            <ul class="navbar-nav mr-auto font-weight-bold " style="font-size: 25px">
                <li class="nav-item active  mr-5 px-5">
                    <a class="nav-link" href="~/Home/Index">Bestill<span class="sr-only">(current)</span></a>
                </li>
                <li class="nav-item active ml-5 px-5">
                    <a class="nav-link" href="~/Home/Linjekart">Linjekart</a>
                </li>


            </ul>


            <li class="form-inline my-2 my-lg-0 nav-item active font-weight-bold" style="font-size: 25px;">

                <a class="nav-link" href="~/Home/Logginn" style="color: black">Logg inn</a>
            </li>
        </div>
    </nav>


    <br />
    
<div class="container body-content ">
    @RenderBody()
    <hr style="margin-top:200px" />
    <footer>
        <p>&copy; @DateTime.Now.Year - ASP.NET Applikasjon laget av: Furkan, Sajan og Zuheb</p>
    </footer>
</div>









</body>
</html>